/* pages/help/help.wxss */
.model{
	height:54px;margin:5px 24px 0;border-bottom: 1px solid #f6f5f5;

}
.model image{
	width: 22px;height:19px;margin-top:18px;margin-right:10px;float: left;
}
.model text{
  line-height: 54px;height: 54px;font-size: 15px;color:#333333;overflow: hidden;text-overflow: ellipsis;width:80%;
white-space:nowrap;
}
.model image:nth-last-of-type(1){
  width:8px;height:13px;margin-top:20px;float:right;
}