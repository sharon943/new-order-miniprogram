.head{
 height: 363px;position: relative;
}
.top_info{
  height: 224px;position: absolute;top:130px;left:10px;width: calc(100% - 20px);background: #fff;border-radius: 5px
}
.top{height: 118px;width: 100%;border-bottom: 1px solid #f5f6f5;}
.left{height: 100%;width: 90px;float: left}
.left .image{
border-radius: 100px;height: 60px;width: 60px;
  margin:30px 20px;overflow: hidden;
}

.right{
  width: calc(100% - 110px);
float: right;
height: 100%;
margin-right: 20px;

}
.right text{
  display: block;white-space: nowrap;
overflow: hidden;
text-overflow: ellipsis;

}
.Rleft{
  float: left;width: 50%;
}
.Rright{
  float: right;width: 90px;
}
.ma{
   width: 34px;
height: 34px;
margin-top: 32px;
margin-left: 48px;

}
.bottom{
width: calc(100% - 50px);
margin:10px 0 0 25px;
}
.bottomTop{
  width: 33.3333333%;
  text-align: center;float: left;
}
.bottomTop image{
  width: 30px;height: 30px;
}
.bottomTop text{
display: block;
}
.name{
  margin-top: 26px;
font-size: 20px;
color: #333333;
font-weight: bold;

}
.mobile{
font-size: 14px;
color: #898989;

}
.leve{
  margin-top: 10px;
  font-size: 10px;
  height: 21px;line-height: 21px;border-radius: 10px;background-color: #d3b095;color: #fff;text-align: center;width: 50px
}
.qy{
  height: 21px;
background-color: #eee;
font-size: 10px;
line-height: 21px;
color: #aaa;
text-align: center;
border-radius: 10px;
margin-top: 15px;

}
.qy text{
  display: inline;width: 74px;
}
.qy image{
  width: 5px;
height: 9px;
margin-left: 5px;
margin-top: 6px;

}
.num{
  color: #d3b095;
  font-size: 18px;
}
.title{
  font-size: 14px;margin-top: 5px;color: #d3b095;
}
.commodityScreenClass_{
  background-color: rgba(0,0,0,0.3);width: 100%;
height: 100%;
position: fixed;
left: 0;
top: 0;
}
.confirm_{
  background: rgb(202,162,132);border: none;
  color: white;
}
.leve_{
   margin:0px 0 0 10px;padding:10px 0;position: relative;
   width: calc(100% - 20px);
   border-radius: 8px;
   background-size: 100%;
  background-image: url("data:image/jpeg;base64,") 
}
.progress_exp{
  position: absolute;top:0;bottom: 0;margin-top:auto;margin-bottom:auto;
}
.progress{
  margin-top:5px;margin-bottom:5px;border-radius:100px;width:60%; text-overflow: ellipsis;white-space: nowrap;overflow:hidden;margin-left:23px
}
.levelName{
  width: 70%;
height: 15px;
font-size: 16px;
margin: 0 0 5px 23px;
}
.service_box{
  background: #fff;margin: 10px 10px 0;width: calc(100% - 60px);height: auto;border-radius: 5px;padding:20px;
}
.box-title{
  height: 22px;margin-bottom: 10px;
}
.line{
  width: 5px;
height: 18px;
border-radius: 10px;
background: #73bba8;
float: left;margin: 2px 5px 0 0;
}
.modle_title{
  float: left
}
.serviceLogoBox{
  display:flex;flex-wrap: wrap
}
.serviceLogoBox > view{
  width:25%;text-align: center;margin-top:10px;
}
.serviceLogoBox image{
  width:50px;height:50px;display: block;margin: 0 auto;
}
.serviceLogoBox view text{
  font-size:12px;color:rgb(155,155,155);
}
.giftImg{
  position: absolute;right:25px;top:28px;width: 60px;height: 60px;
}
.white_back{
	width: calc(100% - 30px);position: fixed;top:10%;left:0;right:0;margin-left:auto;margin-right:auto;z-index: 999;
}
.white_back_text{
	width: 100%;height:95%;background: white;position: relative;
}
.white_back_text > image:nth-child(1) {
width:35vw;height:7.3vw;position: absolute;top:-7.2vw;left:0;right:0;margin-left:auto;margin-right:auto;
}
.white_back_text > view{
	padding:15px 15px 15px;border-top: 1px solid #F7F7F7;z-index: 9999;
}
.share{
	outline:none;border:none;margin-bottom: 30px;background: #fff;
}
.share{
	width: 100%;height:35px;line-height: 35px;border:1px solid #c8ad84;color:#c8ad84;border-radius: 8px;text-align: center;margin-top: 35px;font-size: 16px;background: #fff;
}
.white_back_text > image:nth-of-type(2){
	width: 90px;height:90px;margin:15px auto 15px;display: block;padding-top:20px;
}
.white_back_text text{
	width: 100%;text-align: center;display: block;font-size: 14px;color:#B5B5B5
}
.white_back > image{
	width: 50px;height:50px;position: absolute;left:0;right:0;margin-left:auto;margin-right:auto;bottom: -70px
}